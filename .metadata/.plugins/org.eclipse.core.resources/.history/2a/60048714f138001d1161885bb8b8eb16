package loops;
import java.util.Scanner;
public class NthFibonacciNumber {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int n1 = 1, n2 = 1, sum =0;;
		if(n1 == n ) {
			System.out.println(n1);
			return;
		}if(n == n2 ) {
			System.out.println(n2);
			return;
		}
		for(int i =3; i<=n; i++) {
			int temp = n1+n2;
			n1 = n2;
			n2 = temp;
			sum = temp;
		}
		System.out.println(sum);
	}

}
